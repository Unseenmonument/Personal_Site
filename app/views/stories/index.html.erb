	
	<div class="container-flex yield">
		<div class="row book-row">
			<% @stories.each do |story| %>
				<div class="book">
					<div class="card-title col-sm-2">
						<%= story.title %>
					</div>

					<div class="col-sm-2">
						<div class="more">
							<%= story.story %>
						</div>
					</div>
				</div>
			<% end %>
		</div>	
	</div>
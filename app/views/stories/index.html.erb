	
	<div class="container-flex yield">
		<div class="row book-row">
			<% @stories.each do |story| %>
				<div class="book">
					<div class="row">
						<div class="card-title col-sm-2 book">
							<%= story.title %>
						</div>

						<div class="col-sm-2 book">
							<div class="more">
								<%= story.story %>
							</div>
						</div>
					</div>
				</div>
			<% end %>
		</div>	
	</div>
	
	<div class="container-flex yield">
		<div class="row book-row">
			<% @stories.each do |story| %>
				<div class="book col-sm-2">
					<div class="row">
						<div class="card">
							<div class="card-title book">
								<%= story.title %>
							</div>

							<div class="book">
								<div class="more">
									<%= story.story %>
								</div>
							</div>
						</div>
					</div>
				</div>
			<% end %>
		</div>	
	</div>
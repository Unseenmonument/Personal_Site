	
	<div class="container-flex yield">
		<% @stories.each do |story| %>
			<div class="col-sm-2">
				<ul>
					<li>
						
						<div class="card-title">
							<%= story.title %>
						</div>
					</li>
					<li>
						<div class="more">
							<p>
								<%= story.story %>
							</p>	
						</div>
					</li>
				</ul>
			</div>
		<% end %>
	</div>
<div class="modal-content" id="oops">

      <div class="modal-header">
        <h5 class="modal-title">Let's Get You Signed In!</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body" id="new_user_form">
        <%= render partial: "devise/registrations/new1" %>
      </div>

      <div class="modal-body" id="existing_user_form">  
        <%= render partial: "devise/sessions/new" %>
      </div>

      <div class="modal-footer">
        <div type="button" class="button" id="btn_existing_user"> Already Signed Up? </div>
        <div type="button" class="button" id="btn_non_user"> Need To Sign Up? </div>
      </div>


</div>